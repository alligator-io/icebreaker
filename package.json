{
  "name": "icebreaker",
  "version": "2.0.4",
  "description": "pull-stream based library",
  "main": "index.js",
  "scripts": {
    "build": "npm run browserify && npm run dist",
    "test": "node scripts/test",
    "test-local": "tape test/*.js test/node/*.js",
    "test-browser": "zuul --open --local 8087 -- test/*.js",
    "test-zuul": "zuul -- test/*.js",
    "browserify": "browserify index.js -o dist/icebreaker.js -s icebreaker",
    "dist": "uglifyjs dist/icebreaker.js -o dist/icebreaker.min.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alligator-io/icebreaker.git"
  },
  "keywords": [
    "pull-stream",
    "streams",
    "icebreaker"
  ],
  "author": "Markus Wunderlin",
  "license": "MIT",
  "dependencies": {
    "is-pull-stream": "0.0.0",
    "pull-cat": "^1.1.11",
    "pull-notify": "^0.1.1",
    "pull-pair": "^1.1.0",
    "pull-pushable": "^2.2.0",
    "pull-stream": "^3.6.14",
    "stream-to-pull-stream": "^1.7.3"
  },
  "devDependencies": {
    "zuul": "^3.12.0",
    "browserify": "^16.5.0",
    "tape": "^4.11.0",
    "uglify-js": "^3.6.2"
  }
}
