{
  "name": "icebreaker",
  "version": "1.0.3",
  "description": "pull-stream based library",
  "main": "index.js",
  "scripts": {
    "build": "npm run browserify && npm run dist",
    "test": "node scripts/test",
    "test-local": "tape test/*.js test/node/*.js",
    "test-browser": "zuul --open --local 8087 -- test/*.js",
    "test-zuul": "zuul -- test/*.js",
    "browserify": "browserify lib/icebreaker.js -o dist/icebreaker.js -s icebreaker",
    "dist": "uglifyjs dist/icebreaker.js -o dist/icebreaker.min.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alligator-io/icebreaker.git"
  },
  "keywords": [
    "pull-stream",
    "streams"
  ],
  "author": "Markus Wunderlin",
  "license": "MIT",
  "dependencies": {
    "inherits": "^2.0.1",
    "isarray": "^1.0.0",
    "pull-pair": "^1.1.0",
    "pull-stream": "^3.4.4",
    "stream-to-pull-stream": "^1.7.0"
  },
  "devDependencies": {
    "zuul": "^3.11.0",
    "browserify": "^13.1.0",
    "tape": "^4.6.0",
    "uglify-js": "^2.7.3"
  }
}
